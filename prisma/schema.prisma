generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model game {
  id                  Int      @id @default(autoincrement())
  namespace           String?  @unique
  currentTurn         String   @map("current_turn")
  blackPlayerExists   Boolean? @default(false) @map("black_player_exists")
  whitePlayerExists   Boolean? @default(false) @map("white_player_exists")
  boards              Json     @db.Json
}
